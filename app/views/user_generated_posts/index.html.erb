<p id="notice"><%= notice %></p>

<h1>User Generated Posts</h1>

<table>
  <thead>
    <tr>
      <th>Privacy setting</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_generated_posts.each do |user_generated_post| %>
      <tr>
        <td><%= user_generated_post.privacy_setting %></td>
        <td><%= user_generated_post.text %></td>
        <td><%= link_to 'Show', user_generated_post %></td>
        <td><%= link_to 'Edit', edit_user_generated_post_path(user_generated_post) %></td>
        <td><%= link_to 'Destroy', user_generated_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Generated Post', new_user_generated_post_path %>
